    "jest": "^30.0.5",
    "jose": "^6.0.12",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.453.0",
    "msw": "^2.10.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.6.1",
    "recharts": "^2.15.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^6.0.1",
    "testcafe": "^3.7.2",
    "tsx": "^4.20.3",
    "vite-plugin-inspect": "^0.8.7",
    "vitest": "^3.2.4",
    "wouter": "^3.7.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.4",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "typescript": "5.6.3",
